
server {
        server_name shenfeng.me;
        access_log  /var/log/nginx/blog.access.log;
        location / {
                root /var/www/shenfeng.me;
                index index.html;
                expires 3h;
        }
}


server {
        server_name dict.shenfeng.me;
        location / {
          proxy_pass  http://127.0.0.1:9090/;
          # access_log off;
          # proxy_set_header Host $http_host;
          access_log  /var/log/nginx/dict.access.log;
    }
}
